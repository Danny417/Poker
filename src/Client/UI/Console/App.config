<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IGameService" />
            </basicHttpBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_IGameService" />
            </netTcpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_IGameService" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/poker/basic" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IGameService" contract="GameServiceReference.IGameService"
                name="BasicHttpBinding_IGameService" />
            <endpoint address="http://localhost:8080/poker/ws" binding="wsHttpBinding"
                bindingConfiguration="WSHttpBinding_IGameService" contract="GameServiceReference.IGameService"
                name="WSHttpBinding_IGameService">
                <identity>
                    <servicePrincipalName value="host/asus" />
                </identity>
            </endpoint>
            <endpoint address="net.tcp://localhost:8081/poker" binding="netTcpBinding"
                bindingConfiguration="NetTcpBinding_IGameService" contract="GameServiceReference.IGameService"
                name="NetTcpBinding_IGameService">
                <identity>
                    <servicePrincipalName value="host/asus" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>